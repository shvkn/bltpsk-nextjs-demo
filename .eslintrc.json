{
  "extends": [
    "next/core-web-vitals",
    "eslint-config-next",
    "prettier"
  ],
  "plugins": [
    "unused-imports",
    "simple-import-sort",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "jsxSingleQuote": true
      }
    ],
    "react/prop-types": 0,
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "unused-imports/no-unused-imports": "error"
  }
}
